# Supabase (Database)
SUPABASE_DB_URL=postgresql+psycopg://USER:PASSWORD@HOST:PORT/postgres
SUPABASE_PROJECT_URL=https://xxxxx.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Redis / Workers
REDIS_URL=redis://localhost:6379/0

# KIS API Credentials
KIS_APP_KEY=your_app_key_here
KIS_APP_SECRET=your_app_secret_here
KIS_ACCOUNT_NO=your_account_number_here  # 끝 2자리 상품코드 유의
KIS_USE_SANDBOX=true

# Admin Auth
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000

# Security
ENCRYPTION_KEY=your_32_byte_encryption_key_here  # AES-256

# App Settings
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000
ENVIRONMENT=development
DEBUG=true
